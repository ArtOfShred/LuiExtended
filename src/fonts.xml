    <Font name="LuiExtendedFontinOutline" font="$(LUIE_FONTIN_BOLD)|$(LUIE_18)|thick-outline" />
    <Font name="LuiExtendedRobotoOutline" font="$(LUIE_ROBOTO_BOLD)|$(LUIE_18)|thick-outline" />
