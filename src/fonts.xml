    <!-- Bazooka Font Family -->
    <Font name="LuiExtendedBazookaLarge" font="$(LUIE_BAZOOKA)|$(LUIE_32)|soft-shadow-thick" />
    <Font name="LuiExtendedBazookaMedium" font="$(LUIE_BAZOOKA)|$(LUIE_24)|soft-shadow-thick" />
    <Font name="LuiExtendedBazookaNormal" font="$(LUIE_BAZOOKA)|$(LUIE_18)|soft-shadow-thin" />
    
