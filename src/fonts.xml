<?xml version="1.0" encoding="utf-8"?>
<GuiXml>
    <!-- LUIE Custom Font Faces -->
    <String name="LUIE_ADVENTURE" value="LuiExtended/media/fonts/Adventure/adventure.slug" />
    <String name="LUIE_ARCHIVO_BOLD" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-Bold.slug" />
    <String name="LUIE_ARCHIVO_BOLDITALIC" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-BoldItalic.slug" />
    <String name="LUIE_ARCHIVO_ITALIC" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-Italic.slug" />
    <String name="LUIE_ARCHIVO_MEDIUM" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-Medium.slug" />
    <String name="LUIE_ARCHIVO_MEDIUMITALIC" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-MediumItalic.slug" />
    <String name="LUIE_ARCHIVO_REGULAR" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-Regular.slug" />
    <String name="LUIE_ARCHIVO_SEMIBOLD" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-SemiBold.slug" />
    <String name="LUIE_ARCHIVO_SEMIBOLDITALIC" value="LuiExtended/media/fonts/ArchivoNarrow/ArchivoNarrow-SemiBoldItalic.slug" />
    <String name="LUIE_BAZOOKA" value="LuiExtended/media/fonts/Bazooka/bazooka.slug" />
    <String name="LUIE_COOLINE" value="LuiExtended/media/fonts/Cooline/cooline.slug" />
    <String name="LUIE_DIOGENES" value="LuiExtended/media/fonts/Diogenes/diogenes.slug" />
    <String name="LUIE_ENIGMA_BOLD" value="LuiExtended/media/fonts/Enigma/EnigmaBold.slug" />
    <String name="LUIE_ENIGMA_REGULAR" value="LuiExtended/media/fonts/Enigma/EnigmaReg.slug" />
    <String name="LUIE_FORCED_SQUARE" value="LuiExtended/media/fonts/ForcedSquare/FORCED_SQUARE.slug" />
    <String name="LUIE_FONTIN_BOLD" value="LuiExtended/media/fonts/Fontin/fontin_sans_b.slug" />
    <String name="LUIE_FONTIN_ITALIC" value="LuiExtended/media/fonts/Fontin/fontin_sans_i.slug" />
    <String name="LUIE_FONTIN_REGULAR" value="LuiExtended/media/fonts/Fontin/fontin_sans_r.slug" />
    <String name="LUIE_FONTIN_SMALLCAPS" value="LuiExtended/media/fonts/Fontin/fontin_sans_sc.slug" />
    <String name="LUIE_GINKO" value="LuiExtended/media/fonts/Ginko/ginko.slug" />
    <String name="LUIE_HEROIC" value="LuiExtended/media/fonts/Heroic/heroic.slug" />
    <String name="LUIE_METAMORPHOUS" value="LuiExtended/media/fonts/Metamorphous/metamorphous.slug" />
    <String name="LUIE_PORKY" value="LuiExtended/media/fonts/Porky/porky.slug" />
    <String name="LUIE_PROFONT" value="LuiExtended/media/fonts/ProFontWindows/ProFontWindows.slug" />
    <String name="LUIE_ROBOTO_BOLD" value="LuiExtended/media/fonts/Roboto/Roboto-Bold.slug" />
    <String name="LUIE_ROBOTO_BOLDITALIC" value="LuiExtended/media/fonts/Roboto/Roboto-BoldItalic.slug" />
    <String name="LUIE_TALISMAN" value="LuiExtended/media/fonts/Talisman/talisman.slug" />
    <String name="LUIE_TRAJAN_BOLD" value="LuiExtended/media/fonts/TrajanPro/TrajanProBold.slug" />
    <String name="LUIE_TRANSFORMERS" value="LuiExtended/media/fonts/Transformers/transformers.slug" />
    <String name="LUIE_YELLOWJACKET" value="LuiExtended/media/fonts/Yellowjacket/yellowjacket.slug" />
    
    <!-- Fallback definitions for our custom fonts -->
    <!-- Regular/Medium fonts fallback to MEDIUM_FONT -->
    <FallbackFont font="$(LUIE_ADVENTURE)"/>
    <BackupFont originalFont="$(LUIE_ADVENTURE)" backupFont="$(MEDIUM_FONT)"/>
    
    <!-- Archivo family fallbacks -->
    <FallbackFont font="$(LUIE_ARCHIVO_REGULAR)"/>
    <BackupFont originalFont="$(LUIE_ARCHIVO_REGULAR)" backupFont="$(MEDIUM_FONT)"/>
    <BackupFont originalFont="$(LUIE_ARCHIVO_REGULAR)" backupFont="EsoUI/Common/Fonts/Univers57Cyrillic-Condensed.slug"/>
    
    <FallbackFont font="$(LUIE_ARCHIVO_BOLD)"/>
    <BackupFont originalFont="$(LUIE_ARCHIVO_BOLD)" backupFont="$(BOLD_FONT)"/>
    <BackupFont originalFont="$(LUIE_ARCHIVO_BOLD)" backupFont="EsoUI/Common/Fonts/Univers67Cyrillic-CondensedBold.slug"/>
    
    <!-- Enigma family fallbacks -->
    <FallbackFont font="$(LUIE_ENIGMA_REGULAR)"/>
    <BackupFont originalFont="$(LUIE_ENIGMA_REGULAR)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_ENIGMA_BOLD)"/>
    <BackupFont originalFont="$(LUIE_ENIGMA_BOLD)" backupFont="$(BOLD_FONT)"/>
    
    <!-- Fontin family fallbacks -->
    <FallbackFont font="$(LUIE_FONTIN_REGULAR)"/>
    <BackupFont originalFont="$(LUIE_FONTIN_REGULAR)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_FONTIN_BOLD)"/>
    <BackupFont originalFont="$(LUIE_FONTIN_BOLD)" backupFont="$(BOLD_FONT)"/>
    
    <!-- Roboto family fallbacks -->
    <FallbackFont font="$(LUIE_ROBOTO_BOLD)"/>
    <BackupFont originalFont="$(LUIE_ROBOTO_BOLD)" backupFont="$(BOLD_FONT)"/>
    <BackupFont originalFont="$(LUIE_ROBOTO_BOLD)" backupFont="EsoUI/Common/Fonts/Univers67Cyrillic-CondensedBold.slug"/>
    
    <!-- Decorative fonts fallback to MEDIUM_FONT -->
    <FallbackFont font="$(LUIE_BAZOOKA)"/>
    <BackupFont originalFont="$(LUIE_BAZOOKA)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_COOLINE)"/>
    <BackupFont originalFont="$(LUIE_COOLINE)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_DIOGENES)"/>
    <BackupFont originalFont="$(LUIE_DIOGENES)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_FORCED_SQUARE)"/>
    <BackupFont originalFont="$(LUIE_FORCED_SQUARE)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_GINKO)"/>
    <BackupFont originalFont="$(LUIE_GINKO)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_HEROIC)"/>
    <BackupFont originalFont="$(LUIE_HEROIC)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_METAMORPHOUS)"/>
    <BackupFont originalFont="$(LUIE_METAMORPHOUS)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_PORKY)"/>
    <BackupFont originalFont="$(LUIE_PORKY)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_PROFONT)"/>
    <BackupFont originalFont="$(LUIE_PROFONT)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_TALISMAN)"/>
    <BackupFont originalFont="$(LUIE_TALISMAN)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_TRANSFORMERS)"/>
    <BackupFont originalFont="$(LUIE_TRANSFORMERS)" backupFont="$(MEDIUM_FONT)"/>
    
    <FallbackFont font="$(LUIE_YELLOWJACKET)"/>
    <BackupFont originalFont="$(LUIE_YELLOWJACKET)" backupFont="$(MEDIUM_FONT)"/>
    
    <!-- Standard Font Sizes -->
    <String name="LUIE_8" value="8"/>
    <String name="LUIE_10" value="10"/>
    <String name="LUIE_12" value="12"/>
    <String name="LUIE_14" value="14"/>
    <String name="LUIE_16" value="16"/>
    <String name="LUIE_18" value="18"/>
    <String name="LUIE_20" value="20"/>
    <String name="LUIE_22" value="22"/>
    <String name="LUIE_24" value="24"/>
    <String name="LUIE_26" value="26"/>
    <String name="LUIE_28" value="28"/>
    <String name="LUIE_30" value="30"/>
    <String name="LUIE_32" value="32"/>
    <String name="LUIE_36" value="36"/>
    <String name="LUIE_42" value="42"/>
    <String name="LUIE_48" value="48"/>
    
    <!-- LUIE Custom Font Definitions -->
    <!-- Adventure Font Family -->
    <Font name="LuiExtendedAdventureH1" font="$(LUIE_ADVENTURE)|$(LUIE_30)|soft-shadow-thick" />
    <Font name="LuiExtendedAdventureH2" font="$(LUIE_ADVENTURE)|$(LUIE_24)|soft-shadow-thick" />
    <Font name="LuiExtendedAdventureH3" font="$(LUIE_ADVENTURE)|$(LUIE_20)|soft-shadow-thick" />
    
