    <Font name="LuiExtendedFontinNormal" font="$(LUIE_FONTIN_REGULAR)|$(LUIE_18)|soft-shadow-thin" />
    <Font name="LuiExtendedFontinBold" font="$(LUIE_FONTIN_BOLD)|$(LUIE_18)|soft-shadow-thin" />
    <Font name="LuiExtendedFontinItalic" font="$(LUIE_FONTIN_ITALIC)|$(LUIE_18)|soft-shadow-thin" />
    <Font name="LuiExtendedFontinSmallCaps" font="$(LUIE_FONTIN_SMALLCAPS)|$(LUIE_18)|soft-shadow-thin" />
    
